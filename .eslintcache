 = Global.urlprofesor;\n\n\n    getMessage = (e) => {\n\n        var pages= this.state.currentPage+1;\n\n        axios.get(this.url + 'messages/' + this.state.identity._id+\"/\"+ pages)\n            .then(res => {\n\n                this.setState({\n                    mensaje: res.data.mensajes,\n                    pages:res.data.pages,\n                    status: 'sucess',\n\n                });\n\n            })\n            .catch(err => {\n                this.setState({\n                    mensaje: {},\n                    status: 'failed'\n                });\n            });\n    }\n\n    deleteMessage(id) {\n        axios.delete(this.url + 'delete/' + id)\n            .then(res => {\n                this.setState({\n                    eliminado: res.data.mensaje,\n                    status: 'sucess',\n                });\n                window.location.reload(true);\n            })\n            .catch(err => {\n                this.setState({\n                    eliminado: {},\n                    status: 'failed'\n                });\n            });\n    }\n\n    handlePageClick = mensajes => {\n       \n        const selectedPage = mensajes.selected;\n        const offset = selectedPage * this.state.mensajesPerPage;\n        this.setState({\n            currentPage: selectedPage,\n            offset: offset\n      \n       }, () => \n            this.getMessage());\n        \n    }\n\n    componentDidMount() {\n        this.getMessage();\n    }\n\n    render() {\n\n        let paginationElement;\n\n        if (this.state.pages > 1) {\n            paginationElement = (\n                <ReactPaginate\n                    previousLabel={\"<<\"}\n                    nextLabel={\">>\"}\n                    breakLabel={<span className=\"gap\">...</span>}\n                    pageCount={this.state.pages}\n                    onPageChange={this.handlePageClick}\n                    forcePage={this.state.currentPage}\n                    containerClassNam